{
  "name": "tesis-back",
  "version": "0.0.0",
  "description": "TypeScript + Routing Controllers + Express API Server",
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "start:workers": "ts-node --files src/worker.boot.ts",
    "dev:workers": "cross-env NODE_ENV=development nodemon --exec ts-node --files -r tsconfig-paths/register src/worker.boot.ts"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^0.2.13",
    "@langchain/core": "^0.3.18",
    "ai": "^4.3.13",
    "amqplib": "^0.10.4",
    "axios": "^1.7.7",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.0.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dompurify": "^3.2.2",
    "dotenv": "^10.0.0",
    "envalid": "^7.1.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.1",
    "routing-controllers-openapi": "^2.2.0",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.8.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.7",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.108",
    "@types/amqplib": "^0.10.5",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.11",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.1",
    "@types/jest": "27.4.x",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "27.4.x",
    "lint-staged": "^11.0.0",
    "node-config": "^0.0.2",
    "node-gyp": "^8.1.0",
    "nodemon": "^1.3.3",
    "pm2": "^5.1.0",
    "prettier": "^2.3.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.5.2"
  }
}
